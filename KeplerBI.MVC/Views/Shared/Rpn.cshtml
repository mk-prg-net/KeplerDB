@model KeplerBI.MVC.Models.Filters.Rpn

<li>
    <div class="panel panel-info">
        <div class="panel-heading">
            @Model.Name
        </div>
        <div class="panel-body">
            <dl class="rpn">
                @for (int i = 0; i < Model.ParameterCount; i++)
                {
                    var key = Model.ParameterSubTrees[i].Key;
                    var Name = Model.fn.IsSemanticDescriptor(key) ? Model.fn.ReduceSematicFunctionName(key) : key;

                    <dt>@Name</dt>
                    <dd>@mko.RPN.ParserHelper.ToPNString(Model.ParameterSubTrees[i].Value)</dd>
                }
            </dl>
            <a href="@Url.Action("RpnFunctionEdit", new {controller=Model.ControllerName, pn=Model.pn, pnFSubtree= Model.pnFSubtree})" class="btn btn-primary btn-xs" style="margin-left: 1em;"><span class="glyphicon glyphicon-edit"></span></a>
            <a href="@Url.Action("Index", new {controller=Model.ControllerName, pn=Model.pnWithoutFunction})" class="btn btn-danger btn-xs" style="margin-left: 1em;"><span class="glyphicon glyphicon-remove"></span></a>
        </div>           
     
    </div>

</li>


